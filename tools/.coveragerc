[run]
source = src/tools
omit =
  tests/*,
  fakes.py,
  postprocess.py

[report]
# Exclude Python standard library and site-packages from coverage
exclude_lines =
  pragma: no cover
  def __repr__
  raise AssertionError
  raise NotImplementedError
  if __name__ == .__main__.:
  if TYPE_CHECKING:
  @abstractmethod